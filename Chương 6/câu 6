class StringHandler:
    def __init__(self):
        self.input_string = ""  

    def get_String(self):
        
        self.input_string = input("Nhập một chuỗi: ")

    def print_String(self):
      
        print("Chuỗi in hoa:", self.input_string.upper())
string_handler = StringHandler()
string_handler.get_String()
string_handler.print_String()
